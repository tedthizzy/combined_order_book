      tr
        td #{tbr[7]}
        td #{tbq[7]}
      tr
        td #{tbr[8]}
        td #{tbq[8]}
      tr
        td #{tbr[9]}
        td #{tbq[9]}
      tr
        td #{tbr[10]}
        td #{tbq[10]}
      tr
        td #{tbr[11]}
        td #{tbq[11]}
  #space_vertical
  #container_B2
    #header_B2
      p Volume Plots
    #subhead_B2_bids
      p Bids
    #subhead_B2_highlight
