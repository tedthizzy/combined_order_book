          td #{tpq[4]}
        tr
          td #{tpr[5]}
          td #{tpq[5]}
        tr
          td #{tpr[6]}
          td #{tpq[6]}
        tr
          td #{tpr[7]}
          td #{tpq[7]}
        tr
          td #{tpr[8]}
          td #{tpq[8]}
        tr
          td #{tpr[9]}
          td #{tpq[9]}
        tr
          td #{tpr[10]}
          td #{tpq[10]}
        tr
          td #{tpr[11]}
          td #{tpq[11]}
  #space_horizontal
  #container_C
    p
      | &quot;I&apos;ve been working on a new electronic cash system that&apos;s fully peer-to-peer, with no trusted third party&quot; - Szabo, 2008
script.
  var ilayout = JSON.parse('!{layouti}');
  var idata = JSON.parse('!{datai}');
  Plotly.plot("plotbox_B2", idata, ilayout );
