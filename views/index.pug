doctype html
head
  link(rel='stylesheet' type='text/css' href='css/style.css')
  script(src='https://cdn.plot.ly/plotly-latest.min.js')
#container
  #container_A
    p Combined Order Book for the BTC_ETH Market by Ted Thayer
  #space_horizontal
  #container_B1
    #header_B13
      p #{bit_t} Bittrex Orders
    table
      tr
        th Rate
        th Volume
      tr
        td #{tbr[0]}
        td #{tbq[0]}
      tr
        td #{tbr[1]}
        td #{tbq[1]}
      tr
        td #{tbr[2]}
        td #{tbq[2]}
      tr
        td #{tbr[3]}
        td #{tbq[3]}
